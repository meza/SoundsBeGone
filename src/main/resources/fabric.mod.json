	"authors": [
		"Meza"
	],
	"contact": {
		"homepage": "https://github.com/meza/SoundsBeGone",
		"sources": "https://github.com/meza/SoundsBeGone"
	},
	"license": "GPL-3.0",
	"icon": "assets/soundsbegone/icon.png",
